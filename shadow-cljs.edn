{:source-paths ["src", "assets"]

 :dependencies
 [[reagent "0.10.0"]
  [re-frame "0.12.0"]]

 :dev-http {8080 ["target/", "assets/"]}

 :builds {:app {:target :browser
                :output-dir "target/"
                :asset-path "."
                :modules {:main {:init-fn app.main/main!}}
                :devtools {:after-load app.main/reload!
                           :repl-init-ns app.main
                           :repl-pprint true}}}}
